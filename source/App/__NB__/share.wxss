.social-icon {
	position: relative;
	display: inline-block;
	width: 36px;
	height: 36px;
	font-size: 24px;
	border-radius: 50%;
	line-height: 36px;
	border: 1px solid #666;
	color: #666;
	text-align: center;
	vertical-align: middle;
    transition: background 0.6s ease-out 0s;
    text-decoration: none;
    margin:10px;
}

.social-icon:hover {
	background: #666;
	color: #fff
}

